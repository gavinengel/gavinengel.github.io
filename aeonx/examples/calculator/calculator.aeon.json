{
	"#calculator": {
		".eval": {
			"@onclick": {
				".screen & value": "`$filters.equal`"
			}
		},
		".num": {
			"@on(click)": {
				".screen & value": [".", "`value`"]
			}
		},
		".dec": {
			"@onclick": {
				".screen & value": [".", "`$filters.dot`"]
			}
		},
		".operator": {
			"@onclick": {
				".screen & value": [".", "`$filters.operator`"]	
			}
		},
		".clear": {
			"@onclick": {
				".screen & value": ""
			}
		}
	}
}